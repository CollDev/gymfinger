{% extends 'CollDevMainBundle::layout.html.twig' %}

{% block stylesheets -%}
    {{ parent() }}
    {% stylesheets output='css/fager.css' filter='cssrewrite' debug=false
    'bundles/colldevfager/css/styles.css' -%}
    <link href="{{ asset_url }}" rel="stylesheet" type="text/css" />
    {% endstylesheets %}
{% endblock %}

{% block content -%}{% endblock %}

{% block javascripts -%}
    {{ parent() }}
    {% javascripts output='js/fager.js' debug=false
    '@CollDevFagerBundle/Resources/public/js/scripts.js' -%}
    <script src="{{ asset_url }}"></script>
    {%- endjavascripts -%}
{% endblock %}